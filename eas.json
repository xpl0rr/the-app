{
  "cli": {
    "version": ">= 16.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "internal": {
      "distribution": "internal",
      "channel": "internal"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "release": {
      "developmentClient": false,     
      "distribution": "internal",
      "channel": "release",
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}